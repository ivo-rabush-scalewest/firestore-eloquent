{
  "name": "ivo-rabush-scalewest/firestore-eloquent",
  "description": "Firestore Eloquent, an object-relational mapper (ORM) that makes it enjoyable to interact with your database. When using Eloquent, each firestore collection has a corresponding \\\"Model\\\"/\\\"Class\\\" that is used to interact with that collection. In addition to retrieving records from the firestore collection, Eloquent models/classes allow you to insert, update, and delete records from the collection as well.",
  "type": "library",
  "require": {
    "illuminate/http": "9.* || 10.* || 11.* || 12.*",
    "illuminate/pagination": "9.* || 10.* || 11.* || 12.*",
    "illuminate/support": "9.* || 10.* || 11.* || 12.*",
    "nesbot/carbon": "2.* || 3.*",
    "google/cloud-storage": "1.*",
    "illuminate/session": "9.* || 10.* || 11.* || 12.*",
    "illuminate/config": "9.* || 10.* || 11.* || 12.*",
    "illuminate/filesystem": "9.* || 10.* || 11.* || 12.*",
    "php": ">= 8.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "IvoRabushScalewest\\FirestoreEloquent\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Ivo Rabush",
      "email": "ivo.rabush@scalewest.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "IvoRabushScalewest\\FirestoreEloquent\\Providers\\FModelProvider"
      ],
      "aliases": {
        "FAuth": "IvoRabushScalewest\\FirestoreEloquent\\Auth\\FAuth"
      }
    }
  },
  "repositories": [
    {
        "type": "git",
        "url": "https://github.com/ivo-rabush-scalewest/firestore-eloquent.git"
    }
]
}
